<h1 class="title apps">My Companies</h1>

<div class="col-md-10 comp-container">
  <table class="table table-dark">
    <thead>
  <tr>
  <th scope="col">Company Name</th>
  <th scope="col">Company Website</th>
  <th scope="col">Company Culture</th>
  <th scope="col">Benefits</th>
  <th scope="col">Notes</th>
  </tr>
  </thead>
  {{#each Company}}
  <tbody>
      <tr>
        <th scope="row">{{this.companyName}}</th>

        <td>{{this.website}}</td>
        <td>{{this.culture}}</td>
        <td>{{this.benefits}}</td>
        <td>{{this.notes}}</td>
      </tr>
    </tbody>
          {{/each}}
  </table>
</div>
